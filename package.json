  "license": "MIT",
  "author": "Guyon Roche",
  "repository": {
    "type": "git",
    "url": "https://github.com/guyonroche/exceljs.git"
  },
  "keywords": [
    "xlsx",
    "json",
    "csv",
    "excel",
    "font",
