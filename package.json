{
  "name": "exceljs",
  "version": "0.2.43",
  "description": "Excel Workbook Manager - Read and Write xlsx and csv Files.",
  "private": false,
  "license": "MIT",
  "author": "Guyon Roche",
  "repository": {
    "type": "git",
    "url": "https://github.com/guyonroche/exceljs.git"
  },
  "keywords": [
    "xlsx",
    "json",
    "csv",
    "excel",
    "font",
    "border",
    "fill",
    "number",
    "format",
    "number format",
    "alignment",
    "office",
    "spreadsheet",
    "workbook",
    "defined names",
    "data validations",
    "rich text",
    "in-cell format",
    "outlineLevel",
    "views",
    "frozen",
    "split",
    "pageSetup"
  ],
  "dependencies": {
    "archiver": "0.15.0",
    "bluebird": "*",
    "fast-csv": "0.5.7",
    "jszip": "3.1.3",
    "moment": "2.11.2",
    "promish": "^4.2.8",
    "sax": "^0.6.0",
    "unzip2": "^0.2.5"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "chai": "*",
    "chai-datetime": "*",
    "chai-xml": "*",
    "express": "*",
    "jshint": "*",
    "memorystream": "*",
    "mocha": "*",
    "request": "*",
    "uglifyjs": "^2.4.10",
    "underscore": "1.4.4"
  },
  "scripts": {
    "test": "npm run unit-test && npm run integration-test && npm run end-to-end-test",
    "unit-test": "mocha spec/unit --recursive",
    "integration-test": "mocha spec/integration --recursive",
    "end-to-end-test": "mocha spec/end-to-end --recursive",
    "build": "npm run build-browserify && npm run build-browserify-min",
    "build-browserify": "./node_modules/.bin/browserify browser/index.js --standalone ExcelJS > dist/exceljs.js",
    "build-browserify-min": "./node_modules/.bin/browserify browser/index.js --standalone ExcelJS | ./node_modules/.bin/uglifyjs > dist/exceljs.min.js",
    "browser-test": "node spec/browser/app.js",
    "prepublish": "npm run build"
  },
  "main": "./excel.js",
  "files": [
    "excel.js",
    "lib",
    "browser",
    "dist",
    "LICENSE",
    "README.md",
    "MODEL.md"
  ]
}
